# HostBuild Environment for Lab Environment
# Edit the following
# Hostname for Build
buildhostname=
# Username to Add to Build
username=
# Make user part od SUDOers
sudoers=True
gitpk=True
# Parameters for certificate creation
createcert=True
domain=
ssl_admin=
# Timezone for build
tz=Australia/Brisbane
# Cockpit
inst_cockpit=True
# Packages to add to build
inst_pkgs="git certbot python3-certbot-dns-cloudflare nfs-common python3-pip cockpit nano"
update=True
# Docker Setup
inst_docker=False
inst_dockercompose=False
docker_certdir=/opt/certs
